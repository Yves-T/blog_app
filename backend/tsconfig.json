{
  "compilerOptions": {
    "module": "commonjs",
    "baseUrl": "./",
    "paths": {
      "@routes/*": ["routes/*"],
      "@lib/*": ["lib/*"],
      "@models/*": ["models/*"],
      "@controllers/*": ["controllers/*"],
      "@interface/*": ["interface/*"],
      "@middlewares/*": ["middlewares/*"]
    },
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
